{
  "name": "Groupr-Frontend",
  "version": "1.0.0",
  "description": "Grouper Frontend Javascript project",
  "main": "index.js",
  "repository": "git@github.com:Groupr-Purdue/Groupr-Frontend.git",
  "author": "Alex Aralis <alex.aralis@gmail.com>",
  "license": "MIT",
  "scripts": {
    "post-install": "echo PROJECT_DIR=$(pwd)  >> .defaults",
    "build": "webpack --config dev.webpack.config.js",
    "server": "webpack-dev-server --config dev.webpack.config.js --progress --color --watch --cache --content-base assets/html/ --hot --inline",
    "flow": "flow",
    "flow:watch": "flow-watch",
    "jest": "jest",
    "test:watch": "scripts/test-watch",
    "dev": "yarn run test:watch; yarn run server"
  },
  "dependencies": {
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-flow-react-proptypes": "^0.21.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-plugin-typecheck": "^3.9.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "css-loader": "^0.26.1",
    "enzyme": "^2.7.1",
    "eslint": "^3.14.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-react": "^6.9.0",
    "fetch-mock": "^5.9.3",
    "flexboxgrid": "^6.3.1",
    "flow-bin": "^0.38.0",
    "flow-watch": "^1.1.0",
    "gulp-4.0.build": "^4.0.0-build.a271520",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^3.2.0",
    "jest": "^18.1.0",
    "material-ui": "^0.16.7",
    "mobx": "^3.0.2",
    "mobx-react": "^4.1.0",
    "mobx-react-router": "^3.1.1",
    "node-sass": "^4.4.0",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-flexbox-grid": "^0.10.2",
    "react-grid-layout": "^0.13.9",
    "react-hot-loader": "^1.3.1",
    "react-router": "^3.0.2",
    "react-tap-event-plugin": "^2.0.1",
    "react-test-renderer": "^15.4.2",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  },
  "jest": {
    "automock": false,
    "setupFiles": [
      "./jestrc.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  }
}
